{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/app/build/zephyr/zephyr.elf",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServer",
            "servertype": "jlink",
            "device": "STM32F746ZG",
            "interface": "swd",
            "type": "cortex-debug",
            "request": "launch",
            "armToolchainPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/",
            "gdbPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "rtos": "Zephyr",
            "toolchainPrefix": "arm-zephyr-eabi",
            "runToEntryPoint": "main"
        },
        {
            "name": "Attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/app/build/zephyr/zephyr.elf",
            "serverpath": "/opt/SEGGER/JLink/JLinkGDBServer",
            "servertype": "jlink",
            "device": "STM32F746ZG",
            "interface": "swd",
            "type": "cortex-debug",
            "request": "attach",
            "armToolchainPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/",
            "gdbPath": "${userHome}/zephyr-sdk-0.16.5/arm-zephyr-eabi/bin/arm-zephyr-eabi-gdb",
            "rtos": "Zephyr",
            "toolchainPrefix": "arm-zephyr-eabi"
        },
    ],
}